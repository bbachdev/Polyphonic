{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:default",
    "store:allow-load",
    "store:allow-save",
    "store:allow-get",
    "store:allow-set",
    "store:allow-has",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-keys",
    "store:allow-values",
    "store:allow-entries",
    "store:allow-length",
    "store:allow-reset",
    "sql:default",
    "sql:allow-execute"
  ]
}